<div>
    <h1 class="text-center">Tech Industry News From 'cnet'</h1>
    <hr>
    {{#each hbsObj}}
    {{> article-block}}
    {{/each}}
</div>

{{!-- Modal to add note --}}
<div id="notemodal" class="modal" tabindex="-1" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="modalname">Add a note!</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-row">
                        <label class="field-headers" for="inputName">Name</label>
                        <input type="text" class="form-control" id="inputName" placeholder="Enter your Name here">
                    </div>
                    <div class="form-row">
                        <label class="field-headers" for="inputNote">Comment</label>
                        <textarea rows="5" form="usrform" class="form-control" id="inputNote"
                            placeholder="Add comment.."></textarea>
                    </div>
                    <div class="form-row">
                        <button type="submit" id="addNote" class="btn btn-primary m-1">Add</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

